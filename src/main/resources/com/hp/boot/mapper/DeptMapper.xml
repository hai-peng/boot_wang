<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hp.boot.mapper.DeptMapper">
    <select id="findAllDept" resultType="com.hp.boot.pojo.DeptInfo">
        SELECT * FROM dept
    </select>
    <delete id="delDept">
        DELETE FROM dept where deptid=#{deptid}
    </delete>
</mapper>